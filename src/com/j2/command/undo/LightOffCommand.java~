package com.j2.command.undo;

public class LightOffCommand implements Command {
  Light light;
  int level;
  
  public void LightOffCommand(Light light) {
    this.light = light;
  }
  
  public void execute() {
    level = light.getLevel();
    light.off();
  }
  
  public void undo() {
    light.dim(level);
  }
}
